<?xml version="1.0" encoding="UTF-8"?>
<pipeline xmlns="http://www.esei.uvigo.es/compi-pipeline"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tasks>
		<task id="ID1">
			cmd /c ping -n 2 127.0.0.1 > nul
		</task>
		<foreach id="ID2" dependsOn="ID1" of="list" in="a,a" as="param">
			cmd /c
			ping -n ${param} 127.0.0.1 > nul
		</foreach>
		<task id="ID3" dependsOn="ID2">
			cmd /c ping -n 2 127.0.0.1 > nul
		</task>
		<task id="ID4" dependsOn="ID1">
			cmd /c ping -n 2 127.0.0.1 > nul
		</task>
		<task id="ID5" dependsOn="ID4">
			cmd /c ping -n 2 127.0.0.1 > nul
		</task>
		<task id="ID6" dependsOn="ID3,ID5">
			cmd /c ping -n 2 127.0.0.1 > nul
		</task>
	</tasks>
</pipeline>
