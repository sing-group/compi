<?xml version="1.0" encoding="UTF-8"?>
<pipeline xmlns="http://www.esei.uvigo.es/compi-pipeline" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tasks>
		<task id="ID-1">
				cmd /c echo ${text} > ${destination}
		</task>
	</tasks>
	<params>
		<param name="text" shortName="c" description="The text to write"></param>
		<param name="destination" shortName="d" description="The output file to write into"></param>
	</params>
</pipeline>