<?xml version="1.0" encoding="UTF-8"?>
<pipeline xmlns="http://www.esei.uvigo.es/compi-pipeline" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tasks>
		<task id="ID1">
			echo "hello"
		</task>
		<foreach id="ID2" dependsOn="ID1" of="list" in="-error,-error"
			as="param">
			ls ${param}
		</foreach>
		<task id="ID3" dependsOn="ID2">
			echo "hello"
		</task>
		<task id="ID4" dependsOn="ID1">
			echo "hello"
		</task>
		<task id="ID5" dependsOn="ID4">
			echo "hello"
		</task>
		<task id="ID6" dependsOn="ID3,ID5">
			echo "hello"
		</task>
	</tasks>
</pipeline>
