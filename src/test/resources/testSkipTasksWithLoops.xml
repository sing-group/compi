<?xml version="1.0" encoding="UTF-8"?>
<pipeline xmlns="http://www.esei.uvigo.es/pipeline" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tasks>
		<task id="ID1">
			<foreach element="list" source="2,1,1" as="param" />
			<exec>
				echo ${param}
			</exec>
		</task>
		<task id="ID2" dependsOn="ID1">
			<foreach element="list" source="1,1" as="param" />
			<exec>
				echo ${param}
			</exec>
		</task>
		<task id="ID3" dependsOn="ID1">
			<exec>
				echo "hello"
			</exec>
		</task>
		<task id="ID4" dependsOn="ID2,ID3">
			<foreach element="list" source="1,1,1,1" as="param" />
			<exec>
				echo ${param}
			</exec>
		</task>
	</tasks>
</pipeline>