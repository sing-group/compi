<?xml version="1.0" encoding="UTF-8"?>
<pipeline xmlns="http://www.esei.uvigo.es/compi-pipeline"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tasks>
		<foreach id="ID1" of="list" in="3,2,2" as="param">
			cmd /c ping -n ${param} 127.0.0.1 > nul
		</foreach>
		<foreach id="ID2" dependsOn="ID1" of="list" in="2,2" as="param">
			cmd /c
			ping -n ${param} 127.0.0.1 > nul
		</foreach>
		<task id="ID3" dependsOn="ID1">
			cmd /c ping -n 2 127.0.0.1 > nul
		</task>
		<foreach id="ID4" dependsOn="ID2,ID3" of="list" in="2,2,2,2"
			as="param">
			cmd /c ping -n ${param} 127.0.0.1 > nul
		</foreach>
	</tasks>
</pipeline>