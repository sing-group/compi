<pipeline xmlns="http://www.sing-group.org/compi/pipeline-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <version>1.0</version>
	<tasks>
		<task id="ID1">
			echo "hello"
		</task>
		<foreach id="ID2" after="ID1" of="list" in="-errorj,-errorj"
			as="param">
			ls ${param}
		</foreach>
		<task id="ID3" after="ID2">
			echo "hello"
		</task>
		<task id="ID4" after="ID1">
			echo "hello"
		</task>
		<task id="ID5" after="ID4">
			echo "hello"
		</task>
		<task id="ID6" after="ID3,ID5">
			echo "hello"
		</task>
	</tasks>
</pipeline>
